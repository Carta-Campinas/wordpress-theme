	"styles": {
		"blocks": {
			"core/group": {
				"css": "& news-container {position: relative;} &.news-caption {position: absolute;} &.news-caption,.news-section {padding: var(--wp--custom--news-card--radius);} &.news-caption,.news-section:not(.wide) {border-radius: var(--wp--custom--news-card--radius); overflow: hidden;} &.news-caption {bottom: var(--wp--custom--news-card--caption--bottom);} &.news-item.cover .wp-block-post-featured-image img {height: 100%;} &.news-item .wp-block-post-featured-image {border-radius: var(--wp--custom--news-card--radius); overflow: hidden;} &.news-item.image-top .wp-block-post-featured-image {margin-bottom: 0;} &.news-item.image-top .wp-block-post-title {margin-top: 0.25rem;} &.news-item.image-top .wp-block-tijolo-org-featured-category {position: absolute; bottom: var(--wp--custom--news-card--radius);} &.news-item.image-top .wp-block-tijolo-org-featured-category.has-text-align-left {left: var(--wp--custom--news-card--radius);} &.news-item.image-top .wp-block-tijolo-org-featured-category.has-text-align-right {right: var(--wp--custom--news-card--radius);} &.news-item.image-top .wp-block-tijolo-org-featured-category.has-text-align-center {left: 50%; transform: translateX(-50%);} "
			},
			"core/navigation": {
				"css": "& > .wp-block-navigation__responsive-container {width: 350px; left: auto; right: 0; padding: 0.4em !important;}"
			},
			"core/post-template": {
				"css": ".news-list & {height: 100%; display: flex; flex-direction: column; justify-content: space-between;}"
			},
			"core/post-terms": {
				"css": "&.news-suite {font-size: var(--wp--preset--font-size--small); width: fit-content; margin-left: 0 !important; margin-right: 0 !important;} &.news-list.suite-left {align-self: left;} &.news-list.suite-right {align-self: rignt;} &.news-list.suite-center {align-self: center;}",
				"elements": {
					"link": {
						"color": {
							"background": "var(--wp--preset--color--highlight)",
							"text": "var(--wp--preset--color--base)"
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--display)"
						},
						"css": "& {display: inline-block; padding: 0.1em 0.2em; border-radius: 0.2em; margin: 0.1em;}"
					}
				}
			},
			"core/post-title": {
				"css": "&.news-heading {margin: 0.1em 0; text-shadow: 0 0 0.25em #000000;}"
			},
			"core/site-logo": {
				"css": "header & img {max-height: var(--wp--custom--header-logo-height);}"
			},
			"core/social-links": {
				"css": "&.header-social-menu {padding-left: 10px;}"
			},
			"tijolo-org/featured-category": {
				"color": {
					"background": "var(--wp--preset--color--highlight)"
				},
				"css": "& + .wp-block-post-title {margin-top: 0;}",
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)"
				}
			},
			"hackeamos-org/hackeamos-video-list": {
				"css": "& .video-list-player {border-radius: var(--wp--custom--news-card--radius); overflow: hidden;}"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": "header {margin-bottom: 1rem;} .main-content > *:not(.wide), .main-content > .wide > * {width: calc(100% - 20px); max-width: var(--wp--custom--max-content-width); margin: 0 auto;} @media screen and (max-width: 600px) { .hide-on-mobile { display: none !important; } }",
